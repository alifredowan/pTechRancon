  text print
    //"<a href='"+#servlet+"?page=caform_print&xcontext="+xcontext+"&xform="+xform+"&xtrnnum="+xtrnnum+"&embeddedpage=2' target='Print' class='bt' >Print</a>"
    "<div class='bt' onclick=""PrintElem('printFormID')"">Print</div>"
  
